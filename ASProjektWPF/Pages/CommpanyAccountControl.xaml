<Page x:Class="ASProjektWPF.Pages.CommpanyAccountControl"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ASProjektWPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CommpanyAccountControl">

    <WrapPanel HorizontalAlignment="Center" VerticalAlignment="Center" MaxWidth="500">
        <Button Content="Dodaj kategorie" Click="Btn_AddCategory_Click" Padding="10" Margin="10"/>
        <Button Content="Dodaj poziom stanowiska" Click="Btn_AddPositionLevel_Click" Padding="10" Margin="10"/>
        <Button Content="Dodaj rodzaj umowy" Click="Btn_AddContractType_Click" Padding="10" Margin="10"/>
        <Button Content="Dodaj wymiar pracy" Click="Btn_AddWorkTime_Click" Padding="10" Margin="10"/>
        <Button Content="Dodaj tryb pracy" Click="Btn_AddWorkType_Click" Padding="10" Margin="10"/>
        <Button Content="Dodaj ogłoszenie" Click="Btn_AddAnnouncment_Click" Padding="10" Margin="10"/>
        <Button Content="Edytuj ogłoszenia" Click="Btn_EditAnnouncment_Click" Padding="10" Margin="10"/>
    </WrapPanel>
</Page>
